<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StorageFacility.CreateRackPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Rack Name"/>
            <Entry x:Name="RackNameEntry" Placeholder="Enter rack name"/>
            <Button Text="Create Rack" Clicked="Create_New_Rack"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>