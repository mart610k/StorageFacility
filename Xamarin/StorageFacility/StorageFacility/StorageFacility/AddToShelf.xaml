        <StackLayout>
            <Picker x:Name="shelfPicker"></Picker>
            <Picker x:Name="ProductPicker"></Picker>
            <Button Clicked="AddProductToShelf" Text="Add To Shelf" BackgroundColor="Ivory"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>